package selfProgramming;

public class loopDoWhileExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i =0;
		int sum=0, number =2;
		
		do {
			System.out.println(i + " ");
			i++;	
		} while (i<=3);
		
		do {
			sum+=number;
			number = number + 5;
			System.out.println("SUM :" +sum);	
		}while (number<=10);

	}

}
