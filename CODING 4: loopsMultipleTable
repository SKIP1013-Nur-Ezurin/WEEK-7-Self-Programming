package selfProgramming;

public class loopMultipleTable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		int num2 = 9, j = 1;
		
		//for 
        for(int i = 1; i <= 10; ++i)
        {
            System.out.printf("%d * %d = %d \n", num, i, num * i);
        }
        
        
        //while
        while(j <= 10)
        {
            System.out.printf("%d * %d = %d \n", num2, j, num2 * j);
            j++;
        }
   
	}	   
}





